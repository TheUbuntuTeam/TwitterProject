			#~ user_no_encode = ('user: %s' % (decoded['user']['screen_name'])) 
			#~ user_encoded = ('user: %s' % (decoded['user']['screen_name'].encode('ascii', 'ignore')))
			#~ print(user_no_encode)
			#~ print(user_encoded)
			#~ msg_no_encode = ('tweet: %s' % (decoded['text']))
			#~ msg_encoded = ('tweet: %s' % decoded['text'].encode('ascii', 'ignore'))
			#~ print(msg_no_encode)
			#~ print(msg_encoded)
